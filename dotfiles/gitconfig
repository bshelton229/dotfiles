[alias]
    st = status
    ci = commit
    br = branch
    co = checkout
    df = diff
    dc = diff --cached
    lg = log -p
    who = shortlog -s --
    ps = push
    sm = submodule
    pl = pull
[color]
    ui = true
[core]
	excludesfile = ~/.gitignore_global
	editor = vim
[push]
	default = simple
[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
[include]
    path = ~/.gitconfig-local
